@model ENB.Logistics.Mvc.Models.DisplayPhoneNumber

@{
    ViewBag.Title = "Delete phone-number?";

}
    <div class="alert alert-dismissible alert-danger">
        <h3>Are you sure you want to delete this?</h3>
    </div>
        <div>
            <hr />
            @Html.Partial("_Details", Model)
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                @Html.HiddenFor(x => x.OperatorId)
                <br />
                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-danger" /> |
                    <input type="button" class="btn btn-success" value="Cancel" onclick="location.href='@Url.Action("List", "PhoneNumbers", new { OperatorId= ViewBag.OperatorId })'" />
                </div>
            }
        </div>

